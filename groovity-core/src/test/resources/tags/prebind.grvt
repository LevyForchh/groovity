import com.disney.groovity.ArgsException

prebind(cheese: 123)

assert cheese == 123

cheese = "456"

prebind(cheese: 123)

assert cheese == 456

prebind(cheese: String.class)

assert cheese == '456'

prebind(cheese: null)

assert cheese == '456'

binding.variables.remove('cheese')

