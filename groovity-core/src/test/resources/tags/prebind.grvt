prebind(cheese: null)

assert cheese == null

def argEx
try{
	prebind(cheese: String.class)
}
catch(ArgsException a){
	argEx = a
}
assert argEx != null

prebind(cheese: true)

prebind(cheese: String.class)

assert cheese == 'true'
assert cheese != true

prebind(cheese: Boolean.class)

assert cheese == true
assert cheese != 'true'

argEx = null
try{
	prebind(cheese: 123)
}
catch(ArgsException a){
	argEx = a
}

assert argEx != null

